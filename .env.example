# Copy this to .env and fill in real credentials (DO NOT commit .env)
R2_ENDPOINT=https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
R2_ACCESS_KEY=YOUR_ACCESS_KEY
R2_SECRET_KEY=YOUR_SECRET_KEY
R2_BUCKET=vercel-clone-storage

# Postgres (used by upload-service & build-service)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=vercel_clone
# JWT configuration
JWT_SECRET=your-super-secret-key-change-this-in-production-at-least-256-bits-long
JWT_EXPIRATION=86400000